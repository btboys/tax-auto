{
  "name": "tax-auto",
  "version": "1.0.0",
  "description": "税务多开操作小助手",
  "main": "./out/main/index.js",
  "author": "夏悸",
  "license": "Apache-2.0",
  "homepage": "https://fenxi365.com",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "yarn run typecheck:node && yarn run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "yarn run typecheck && yarn run rebuild-playwright && electron-vite build",
    "rebuild-playwright": "electron-rebuild -f -w playwright",
    "build:unpack": "yarn run build && electron-builder --dir",
    "build:win": "yarn run build && electron-builder --win --x64",
    "build:mac": "yarn run build && electron-builder --mac --x64",
    "build:linux": "yarn run build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@layui/layer-vue": "^2.4.7",
    "electron-updater": "^6.3.9",
    "localforage": "^1.10.0",
    "pinia": "^3.0.1",
    "playwright": "^1.50.1",
    "radash": "^12.1.0",
    "tdesign-icons-vue-next": "^0.3.4",
    "tdesign-vue-next": "^1.11.1"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/node": "^22.13.4",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "autoprefixer": "^10.4.20",
    "electron": "^34.2.0",
    "electron-builder": "^25.1.8",
    "electron-vite": "^3.0.0",
    "eslint": "^9.20.1",
    "eslint-plugin-vue": "^9.32.0",
    "less": "^4.2.1",
    "postcss": "^8.4.49",
    "prettier": "^3.5.1",
    "strip-ansi": "^7.1.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.2"
  }
}
